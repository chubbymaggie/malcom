# malcom.conf
# The default malcom configuration

[web]
activated = true
listen_interface = 0.0.0.0
listen_port = 8080
auth = false

[feeds]
activated = true
feeds_dir = feeds
scheduler = false

[analytics]
activated = true
max_workers = 12

[sniffer]
activated = true
network = true
sniffer_dir = sniffer
tls_proxy_port = 9000
yara_path = yara

# databases can be configured here

[db_local]
name = Local
type = db
host = localhost
port = 27017


[feeds]
# You can comment / uncomment feeds
# according to if you want to use
# them or not

AsproxTracker
CybercrimeTracker
FeodoTracker
MalcodeBinaries
MalwaredRu
MalwareTrafficAnalysis
MalwareDomainList
PalevoTracker
TorExitNodes
ZeusTrackerBinaries
ZeusTrackerConfigs
ZeusTrackerDropzones

